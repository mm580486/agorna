
<main>

 <div class="row">
      <div class="col s12 m12">
          
        <div class="card-panel white">
        
 <div class="row">
    
    
    
    <table class="responsive-table striped">
              <thead>
                <tr>
                    <th>ویرایش</th>
                    <th>ادرس</th>
                    <th>
                      تلفن
                    </th>
                    <th>فروشگاه</th>
                    <th>
                      نام
                    </th>
                </tr>
              </thead>
              <tbody>
                <% @tasks.each do |task| %>
                <tr>
                  <td>
                                      <%= link_to(raw("<i class='material-icons'>#{task.seller.exposition_accept? ? 'lock_open':'lock_outline'}</i>"),toggle_lock_admin_seller_path(task.seller.id),class: "small btn-floating waves-effect btn-small waves-light  #{task.seller.exposition_accept? ? 'green':'yellow' }") %>
                  </td>
                  <td><%= task.seller.exposition_address %></td>
                  <td><%= task.seller.phone %></td>
                  <td><%= task.seller.exposition_name %></td>
                  <td><%= task.seller.name %></td>
                </tr>
                <% end %>
                
              </tbody>
            </table>
            
            
    
  </div>
        
        
        
            
            
          
          
        </div>
      </div>
 </div>

</main>