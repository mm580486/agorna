

<%# content_for :head do %>
  
  
<%# end %>
<main>

 <div class="row">
      <div class="col s12 m12">
          
        <div class="card-panel white">

        <table class='striped highlight bordered'>
            <thead>
              <tr>
                  <th>ویرایش</th>
                 
                  <th>لینک</th>
  <th>نام درخواست کننده</th>
              </tr>
            </thead>
    
            <tbody>
            <% @chains.each do |chain| %>
              <tr>
                <td>
                  <%= link_to(raw("<i class='material-icons'>#{chain.accept? ? 'lock_outline':'lock_open'}</i>"),toggle_lock_admin_seller_chain_path(chain.id),class: 'small btn-floating waves-effect btn-small waves-light green') %>
                </td>
                <td><%= link_to chain.user.exposition_name,show_exposition_path(chain.user.identify),terget: '_blank' %></td>
                <td><%= chain.user.name %></td>
              </tr>
            <% end %>
            </tbody>
          </table>
        </div>
      </div>
 </div>
 <%= link_to(raw('<i class="material-icons">add</i>'),new_admin_seller_chain_path,class: 'btn-floating btn-large waves-effect waves-light red',style: 'position:fixed;bottom: 20px; left: 24px;') %>
</main>